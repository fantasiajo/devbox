{
    "workbench.colorTheme": "Default Dark+",
    
    "errorLens.excludeBySource": [
        "cSpell"
    ],

    "local-history.path": "~/code/.history",

    "go.testFlags": [
        "-v",
        "-count=1"
    ],
    "go.lintTool": "golangci-lint",
    "go.testTimeout": "3600s",
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 1024,
            "maxArrayValues": 64,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": true
    }
}